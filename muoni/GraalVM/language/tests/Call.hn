fn ret(a) { return a; }
fn dub(a) { return a * 2; }
fn inc(a) { return a + 1; }
fn dec(a) { return a - 1; }
fn call(f, v) { return f(v); }
 
fn main() {
  println(ret(42));
  println(dub(21));
  println(inc(41));
  println(dec(43));
  println(call(ret, 42));
  println(call(dub, 21));
  println(call(inc, 41));
  println(call(dec, 43));
}  
